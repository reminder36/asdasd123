
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "ANIME BATTLES SIMULATOR SCRIPT | BY NAMELESS SCRIPTS | DISCORD: NS HUB" .. Fluent.Version,
    SubTitle = "",
    TabWidth = 150,
    Size = UDim2.fromOffset(570, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Aqua",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "house" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Tabs = {
    Main = Window:AddTab({ Title = "Auto Farm", Icon = "swords" }),
}

local Options = Fluent.Options

do
    Fluent:Notify({
        Title = "Notification",
        Content = "This is a notification",
        SubContent = "SubContent", -- Optional
        Duration = 5 -- Set to nil to make the notification not disappear
    })


    local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "Auto Attack | Gain Power", Default = false })
    local function checkToggle()
        while Toggle.Value do
            local args = {
                [1] = "attack"
            }
            
            game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))            
            print("Toggle changed:", Options.MyToggle.Value)
            wait() -- Add a delay to avoid spamming the server with too many events
        end
    end
    
    Toggle:OnChanged(checkToggle)
    checkToggle()

    local Players = game:GetService("Players")
local player = Players.LocalPlayer
local humanoidRootPart = player.Character:WaitForChild("HumanoidRootPart")

local workspace = game:GetService("Workspace")
local spawnersFolder = workspace:FindFirstChild("spawners")
local namequiFolder = spawnersFolder and spawnersFolder:FindFirstChild("namequi")

local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "NAMEQUI MAP", Default = false })

local function teleportEnemies()
    if namequiFolder then
        for _, enemyPart in pairs(namequiFolder:GetChildren()) do
            if enemyPart:IsA("Part") then
                -- Teleport to the position of each enemy part
                humanoidRootPart.CFrame = CFrame.new(enemyPart.Position + Vector3.new(0, 5, 0)) -- Teleport 5 studs above the enemy
                -- You can adjust the teleportation position as needed
                wait(1.5)  -- Add a delay between teleports (adjust as needed)
            end
        end
    else
        warn("Namequi folder not found in Spawners.")
    end
end

local function checkToggle()
    if Toggle.Value then
        print("Teleportation enabled.")
        teleportEnemies()
    else
        print("Teleportation disabled.")
    end
end

Toggle:OnChanged(checkToggle)
checkToggle()

local Players = game:GetService("Players")
local player = Players.LocalPlayer
local humanoidRootPart = player.Character:WaitForChild("HumanoidRootPart")

local workspace = game:GetService("Workspace")
local spawnersFolder = workspace:FindFirstChild("spawners")
local komonaFolder = spawnersFolder and spawnersFolder:FindFirstChild("komona")

local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "KOMOHA MAP", Default = false })

local function teleportEnemies()
    if komonaFolder then
        for _, enemyPart in pairs(komonaFolder:GetChildren()) do
            if enemyPart:IsA("Part") then
                -- Teleport to the position of each enemy part
                humanoidRootPart.CFrame = CFrame.new(enemyPart.Position + Vector3.new(0, 5, 0)) -- Teleport 5 studs above the enemy
                -- You can adjust the teleportation position as needed
                wait(1.5)  -- Add a delay between teleports (adjust as needed)
            end
        end
    else
        warn("komoha folder not found in Spawners.")
    end
end

local function checkToggle()
    if Toggle.Value then
        print("Teleportation enabled.")
        teleportEnemies()
    else
        print("Teleportation disabled.")
    end
end

Toggle:OnChanged(checkToggle)
checkToggle()

