    local Tabs = {
        Main = Window:AddTab({ Title = "AUTO BUY", Icon = "store" }),
    }

    local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "STYLES", Default = false })
    local function checkToggle()
        while Toggle.Value do
            local args = {
                [1] = "spin",
                [2] = "styles",
                [3] = "one",
                [4] = "namequi"
            }
            
            game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))            
            print("Toggle changed:", Options.MyToggle.Value)
            wait() -- Add a delay to avoid spamming the server with too many events
        end
    end
    
    Toggle:OnChanged(checkToggle)
    checkToggle()

    local Tabs = {
        Main = Window:AddTab({ Title = "SPIN PET", Icon = "star" }),
    }

    local isToggleOn = false
    local selectedValue = ""
    
    -- Dropdown creation block
    local Dropdown = Tabs.Main:AddDropdown("Dropdown", {
        Title = "Select Option to Auto Hatch",
        Values = {"NAMEQUI EGG", "KOMONA EGG"},
        Multi = false,
        Default = 1,
    })
    
    Dropdown:SetValue("Tap Here to Select")
    
    Dropdown:OnChanged(function(Value)
        selectedValue = Value
        print("Dropdown changed:", selectedValue)
    end)
    
    -- Toggle creation block
    local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "Auto Hatch Selected", Default = false })
    
    -- Function to handle actions based on toggle state
    local function HandleToggleAction()
        while isToggleOn do
            print("Toggle changed:", Toggle.Value)
    
            if isToggleOn then
                if selectedValue == "NAMEQUI EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "namequi"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))                    
                elseif dropdownValue == "KOMONA EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "komona"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "TOKYO EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "tokyo"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "SHELL VILLAGE EGG" 
                local args = {
                    [1] = "spin",
                    [2] = "champions",
                    [3] = "one",
                    [4] = "shell village"
                }
                
                game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "HUECO MUNDO EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "hueco mundo"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "SARIUH EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "sariuh"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "JUTSO EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "jutso"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "KINTETSU EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "kintetsu"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "DARK CLOVER EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "dark clover"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                elseif dropdownValue == "TITAN ATTACK EGG" then
                    local args = {
                        [1] = "spin",
                        [2] = "champions",
                        [3] = "one",
                        [4] = "titan attack"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                else
                    print("Toggle is ON and unexpected option selected")
                end
            end
            
            wait()
            end
            end
            
            Toggle:OnChanged(function(value)
            isToggleOn = value
            print("Toggle is now:", isToggleOn)
            
            if isToggleOn then
            -- Start the toggle action in a new thread to not block the UI
            spawn(HandleToggleAction)
            end
            end)
            
            if Toggle.Value then
            -- Start the toggle action initially
            spawn(HandleToggleAction)
            end

            local Tabs = {
                Main = Window:AddTab({ Title = "AUTO QUEST", Icon = "list" }),
            }

            local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "NAMEQUI MAP", Default = false })
            local function checkToggle()
                while Toggle.Value do
                    local args = {
                        [1] = "set_quest",
                        [2] = true,
                        [3] = "namequi"
                    }
                    
                    game:GetService("ReplicatedStorage").Shared.events.RemoteEvent:FireServer(unpack(args))
                      
                    print("Toggle changed:", Options.MyToggle.Value)
                    wait(2) -- Add a delay to avoid spamming the server with too many events
                end
            end
            
            Toggle:OnChanged(checkToggle)
            checkToggle()

            local Toggle = Tabs.Main:AddToggle("MyToggle", {Title = "KOMONA MAP", Default = false })
            local function checkToggle()
                while Toggle.Value do
                    local args = {
                        [1] = "set_quest",
